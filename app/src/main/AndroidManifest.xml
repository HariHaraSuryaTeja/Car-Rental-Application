<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.finalproject.carrentalsv3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/poster2_15_105310"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        android:usesCleartextTraffic="true"
        <activity android:name=".RegisterPage"></activity>
        <activity android:name=".LoginPage" />
        <activity android:name=".PickupAndDropOff" />
        <activity android:name=".PaymentPage" />
        <activity android:name=".CitySelection" />
        <activity android:name=".CarConfirmation" />
        <activity android:name=".BookingConfirmation" />
        <activity android:name=".CarSelection" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>